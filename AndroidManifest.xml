<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.studybank">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">



        <activity android:name=".HomepageActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoginActivity"></activity>

        <activity android:name=".ProfileMainActivity"></activity>
        <activity android:name=".ProfileChangeActivity"/>
        <activity android:name=".FriendsList"/>
        <activity android:name=".UserScore"/>
        <activity android:name=".QuizLibrary"/>

        <activity android:name=".CreateActivity"/>
        <activity android:name=".MultipleChoiceActivity"/>
        <activity android:name=".TrueFalseActivity"/>

        <activity android:name=".QuizActivity"></activity>

        <activity android:name="ImExActivity"></activity>

    </application>

</manifest>